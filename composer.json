{
    "name": "bgies/translation-manager-laravel",
    "description": "Translation Manager for Laravel. Makes managing translations easier",
    "keywords": [
        "Translation",
        "laravel",
        "Translate",
        ":package_name"
    ],
    "homepage": "https://github.com/bgies/translation-manager-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Brad Gies",
            "email": "rbgies@gmail.com",
            "homepage": "http://bgies.oom",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~4|~5",
        "php" : ">=5.3.0",
        "illuminate/filesystem": "~4|~5"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Bgies\\TranslationEditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bgies\\TranslationEditor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
